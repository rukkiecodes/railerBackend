<v-navigation-drawer v-model="drawer" color="deep-purple accent-4" mini-variant  app>
  <v-list dense>
    <v-list-item dense v-for="(route, i) in routes" :key="i" :href="route.to" link>
      <v-list-item-icon>
        <v-icon>{{ route.icon }}</v-icon>
      </v-list-item-icon>
      <v-list-item-title>{{ route.title }}</v-list-item-title>
    </v-list-item>
  </v-list>
</v-navigation-drawer>