<% for(let template of templates ){ %>
<v-hover v-slot="{ hover }">
  <v-card class="card mb-6 mb-sm-4 rounded-lg" color="grey lighten-4">
    <v-img src="<%= template.image %>">
      <v-expand-transition>
        <v-card v-if="hover" class="pa-3 transition-fast-in-fast-out v-card--reveal" style="height: 100%;">
          <vs-button block size="small" color="#6200EA" class="gs rounded-lg">
            Get started
          </vs-button>
          <vs-button block size="small" color="#757575" class="rounded-lg ">
            Preview
          </vs-button>
        </v-card>
      </v-expand-transition>
    </v-img>
  </v-card>
</v-hover>
<% } %>